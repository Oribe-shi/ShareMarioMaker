{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' https://cdn.discordapp.com; connect-src 'self' https://discordsays.com;"
                }
            ]
        }
    ]
}
